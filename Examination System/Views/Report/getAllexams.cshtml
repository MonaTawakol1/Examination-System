@model IEnumerable<Exam>
@{
    ViewData["Title"] = "Reports5";
}

<h1>Reports5</h1>


<table class="table table-bordered">
    <tr>


        <th>exam </th>
        <th>opration </th>


    </tr>
    @foreach (var item in Model)
    {
        <tr>


            <td>
                @item.ExamId
               
            </td>
            <td>
                <a class="btn btn-primary" asp-action="Reports6" asp-controller="Report" asp-route-ExamId="@item.ExamId">Go To Exam</a>

            </td>
        </tr>
    }
</table>