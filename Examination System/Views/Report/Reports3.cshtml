@model IEnumerable<Course>
@{
    ViewData["Title"] = "Reports3";
}


<h1>Reports3</h1>


<table class="table table-bordered">
    <tr>
    
        <th> Course Name</th>
        <th>Students Number</th>


    </tr>
    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @item.CourseName
            </td>
            <td>
                @item.Students.Count
            </td>
        </tr>
    }
</table>