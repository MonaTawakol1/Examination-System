@model IEnumerable<Topic>
@{
    ViewData["Title"] = "Reports4";
}

<h1>Reports4</h1>


<table class="table table-bordered">
    <tr>

       
        <th>Topics Name</th>


    </tr>
    @foreach (var item in Model)
    {
        <tr>

          
            <td>
                @item.TopicName
            </td>
        </tr>
    }
</table>